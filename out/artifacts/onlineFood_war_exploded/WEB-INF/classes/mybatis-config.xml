<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--配置数据源，交给Spring去做    -->
    <typeAliases>
        <package name="com.yuan.domain"/>
    </typeAliases>

<!--    绑定BookMapper.xml 映射配置-->
    <mappers>
        <mapper class="com.yuan.dao.UserMapper"/>
        <mapper class="com.yuan.dao.FoodMapper"/>
        <mapper class="com.yuan.dao.OrderMapper"/>
        <mapper class="com.yuan.dao.AdminMapper"/>
        <mapper class="com.yuan.dao.MessageMapper"/>
    </mappers>
</configuration>